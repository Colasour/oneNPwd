.cell {
  box-sizing: border-box;
  padding: 15rpx 70rpx;
  display: flex;
  width: 100vw;
}

.cell-wrapper {
  box-sizing: border-box;
  padding: 30rpx;
  display: flex;
  align-items: center;
  width: 100%;
  border-radius: 12rpx;
}

.cell-tag {
  margin: 0 20rpx;
  padding: 2rpx 10rpx;
  font-size: 24rpx;
  border-radius: 10rpx;
}

.cell-field {
  flex: 1;
  margin: 0 10rpx;
  font-size: 30rpx;
  letter-spacing: 2rpx;
}

.cell-ellipsis {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.bg-opacity {
  opacity: 0.7;
}
